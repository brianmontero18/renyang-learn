####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2 -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT -DQT_SHARED -DQT_TABLET_SUPPORT
CXXFLAGS = -pipe -Wall -W -O2 -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT -DQT_SHARED -DQT_TABLET_SUPPORT
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I/usr/share/qt3/mkspecs/default -I. -I/usr/include/qt3
LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L/usr/share/qt3/lib -L/usr/X11R6/lib -lqt-mt -lXext -lX11 -lm -lpthread
AR       = ar cqs
RANLIB   = 
MOC      = /usr/share/qt3/bin/moc
UIC      = /usr/share/qt3/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -r
INSTALL_FILE= $(COPY_FILE)
INSTALL_DIR = $(COPY_DIR)
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = ./

####### Files

HEADERS = 
SOURCES = main.cpp
OBJECTS = main.o \
		tripplanner.o
FORMS = tripplanner.ui
UICDECLS = tripplanner.h
UICIMPLS = tripplanner.cpp
SRCMOC   = moc_tripplanner.cpp
OBJMOC = moc_tripplanner.o
DIST	   = tripplanner.pro
QMAKE_TARGET = tripplanner
DESTDIR  = 
TARGET   = tripplanner

first: all
####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: tripplanner.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile tripplanner.pro
qmake: 
	@$(QMAKE) -o Makefile tripplanner.pro

dist: 
	@mkdir -p .tmp/tripplanner && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/tripplanner/ && $(COPY_FILE) --parents tripplanner.ui.h .tmp/tripplanner/ && ( cd `dirname .tmp/tripplanner` && $(TAR) tripplanner.tar tripplanner && $(GZIP) tripplanner.tar ) && $(MOVE) `dirname .tmp/tripplanner`/tripplanner.tar.gz . && $(DEL_FILE) -r .tmp/tripplanner

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

main.o: main.cpp tripplanner.h

tripplanner.h: tripplanner.ui 
	$(UIC) tripplanner.ui -o tripplanner.h

tripplanner.cpp: tripplanner.h tripplanner.ui tripplanner.ui.h 
	$(UIC) tripplanner.ui -i tripplanner.h -o tripplanner.cpp

tripplanner.o: tripplanner.cpp tripplanner.h

moc_tripplanner.o: moc_tripplanner.cpp  tripplanner.h 

moc_tripplanner.cpp: $(MOC) tripplanner.h
	$(MOC) tripplanner.h -o moc_tripplanner.cpp

####### Install

install:  

uninstall:  

