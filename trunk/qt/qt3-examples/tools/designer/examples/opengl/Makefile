	@mkdir -p .obj/debug-shared-mt/glwidget && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/debug-shared-mt/glwidget/ && ( cd `dirname .obj/debug-shared-mt/glwidget` && $(TAR) glwidget.tar glwidget && $(GZIP) glwidget.tar ) && $(MOVE) `dirname .obj/debug-shared-mt/glwidget`/glwidget.tar.gz . && $(DEL_FILE) -r .obj/debug-shared-mt/glwidget

