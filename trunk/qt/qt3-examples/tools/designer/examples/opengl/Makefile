$(MOC): 
	( cd /usr/share/qt3/src/moc && $(MAKE) )

Makefile: opengl.pro ../../../../.qmake.cache /usr/share/qt3/mkspecs/linux-g++/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile opengl.pro
qmake: 
	@$(QMAKE) -o Makefile opengl.pro

