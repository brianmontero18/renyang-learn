blob/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "blob" || $(MKDIR) "blob"
	cd blob && $(QMAKE) blob.pro -o $(MAKEFILE)
sub-blob: blob/$(MAKEFILE) FORCE
	cd blob && $(MAKE) -f $(MAKEFILE)

