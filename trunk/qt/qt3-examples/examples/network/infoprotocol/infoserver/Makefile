	$(UIC) serverbase.ui -i serverbase.h -o serverbase.cpp

.obj/debug-shared-mt/serverbase.o: serverbase.cpp serverbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/debug-shared-mt/serverbase.o serverbase.cpp

