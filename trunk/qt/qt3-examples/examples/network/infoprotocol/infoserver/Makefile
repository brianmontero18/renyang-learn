	@mkdir -p .obj/debug-shared-mt/infoserver && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/debug-shared-mt/infoserver/ && ( cd `dirname .obj/debug-shared-mt/infoserver` && $(TAR) infoserver.tar infoserver && $(GZIP) infoserver.tar ) && $(MOVE) `dirname .obj/debug-shared-mt/infoserver`/infoserver.tar.gz . && $(DEL_FILE) -r .obj/debug-shared-mt/infoserver

