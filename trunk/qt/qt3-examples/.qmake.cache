CONFIG +=  nocrosscompiler dlopen_opengl minimal-config small-config medium-config large-config full-config styles tools kernel widgets dialogs iconview workspace inputmethod network canvas table xml opengl sql opengl debug dll thread largefile stl ipv6 system-mng system-jpeg jpeg system-png png gif system-zlib nis cups nas bigcodecs x11sm xshape xinerama xcursor xrandr xrender xftfreetype tablet xkb inputmethod dylib create_prl link_prl
QMAKESPEC = linux-g++
QT_SOURCE_TREE = /usr/share/qt3
QT_BUILD_TREE = /usr/share/qt3
QMAKE_ABSOLUTE_SOURCE_ROOT = $$QT_SOURCE_TREE
QT_PRODUCT=qt-free
styles += windows motif mac platinum sgi cde motifplus
kbd-drivers += tty
mouse-drivers += pc
sql-plugins += odbc
sql-plugins += psql
sql-plugins += mysql
sql-plugins += sqlite
QMAKE_MOC=/usr/share/qt3/bin/moc
QMAKE_UIC=/usr/share/qt3/bin/uic -L $$QT_BUILD_TREE/plugins
QMAKE_QMAKE=/usr/share/qt3/bin/qmake
QMAKE_MOC_SRC=/usr/share/qt3/src/moc
INCLUDEPATH+=/usr/include/freetype2
QMAKE_LIBS_X11=-lXft -lfreetype -lfontconfig $$QMAKE_LIBS_X11
QMAKE_INCDIR_QT=/usr/share/qt3/include
QMAKE_LIBDIR_QT=/usr/share/qt3/lib
QMAKE_RTLDIR_QT=/usr/lib
OBJECTS_DIR=.obj/debug-shared-mt
MOC_DIR=.moc/debug-shared-mt
QMAKE_LIBS_QT+=-ljpeg
QMAKE_LIBS_QT_THREAD+=-ljpeg
QMAKE_LIBS_QT+=-lpng
QMAKE_LIBS_QT_THREAD+=-lpng
QMAKE_LIBS_QT+=-lz
QMAKE_LIBS_QT_THREAD+=-lz
QMAKE_LIBS_X11 = -lXinerama $$QMAKE_LIBS_X11
QMAKE_LIBS_X11 = -lXcursor $$QMAKE_LIBS_X11
QMAKE_LIBS_X11 = -lXrandr $$QMAKE_LIBS_X11
QMAKE_LIBS_X11 = -lXrender $$QMAKE_LIBS_X11
QMAKE_LIBS_X11 = -lXi $$QMAKE_LIBS_X11
LIBS+= -lfontconfig
QMAKE_CFLAGS+= -I/usr/include/mysql -I/usr/include/freetype2 -I/usr/include/postgresql
QMAKE_CXXFLAGS+= -I/usr/include/mysql -I/usr/include/freetype2 -I/usr/include/postgresql
QT_INSTALL_PREFIX=/usr
docs.path=/usr/share/qt3/doc
headers.path=/usr/include/qt3
plugins.path=/usr/lib/qt3/plugins
libs.path=/usr/lib
bins.path=/usr/bin
data.path=/usr/share/qt3
translations.path=/usr/share/qt3/translations
QMAKE_CFLAGS+=-fno-exceptions
QMAKE_CXXFLAGS+=-fno-exceptions
QMAKE_LFLAGS+=-fno-exceptions
