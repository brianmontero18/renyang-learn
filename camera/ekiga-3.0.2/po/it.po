# GNOME 2.24
#: ../ekiga.schemas.in.in.h:149
msgid ""
"This allows you to set the mode for sending DTMFs. The values can be "
"\"RFC2833\" (0), \"INFO\" (1)"
msgstr ""
"Consente di impostare la modalità di invio DTMF. I valori possono essere "
"\"RFC2833\" (0), \"INFO\" (1)"

# GNOME 2.24
