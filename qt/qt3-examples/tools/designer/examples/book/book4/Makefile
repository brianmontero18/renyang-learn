book.h: book.ui book.ui.h
	$(UIC) book.ui -o book.h

book.cpp: book.h book.ui book.ui.h book.ui.h
	$(UIC) book.ui -i book.h -o book.cpp

.obj/debug-shared-mt/book.o: book.cpp book.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/debug-shared-mt/book.o book.cpp

