$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: downloader.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile downloader.pro
