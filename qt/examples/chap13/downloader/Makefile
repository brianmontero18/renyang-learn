dist: 
	@mkdir -p .tmp/downloader && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/downloader/ && ( cd `dirname .tmp/downloader` && $(TAR) downloader.tar downloader && $(GZIP) downloader.tar ) && $(MOVE) `dirname .tmp/downloader`/downloader.tar.gz . && $(DEL_FILE) -r .tmp/downloader

