	@$(QMAKE) -o Makefile dragdrop_canvas.pro

dist: 
	@mkdir -p .tmp/dragdrop_canvas && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/dragdrop_canvas/ && ( cd `dirname .tmp/dragdrop_canvas` && $(TAR) dragdrop_canvas.tar dragdrop_canvas && $(GZIP) dragdrop_canvas.tar ) && $(MOVE) `dirname .tmp/dragdrop_canvas`/dragdrop_canvas.tar.gz . && $(DEL_FILE) -r .tmp/dragdrop_canvas

