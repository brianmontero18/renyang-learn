$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: dragdrop_canvas.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile dragdrop_canvas.pro
